{
  "name": "ycms/framework",
  "description": "Laravel CMS Based by YASCMF",
  "keywords": [
    "framework",
    "ycms",
    "laravel"
  ],
  "license": "WTFPL",
  "type": "project",
  "require": {
    "php": ">=5.5.9",
    "laravel/framework": "5.1.*",
    "zizaco/entrust": "dev-laravel-5",
    "zhuzhichao/ip-location-zh": "~1.2",
    "barryvdh/laravel-debugbar": "~2.0",
    "douyasi/identity-card": "dev-master",
    "doctrine/dbal": "^2",
    "jenssegers/mongodb": "dev-master",
    "jenssegers/mongodb-session": "dev-master",
    "laravel/envoy": "dev-master",
    "barryvdh/laravel-ide-helper": "^2.0@dev",
    "vinkla/hashids": "^2.0@dev",
    "lord/laroute": "2.*",
    "illuminate/html": "^5.0@dev",
    "ycms/modules": "dev-master",
    "ycms/krumo": "^0.5.0",
    "ycms/tpl": "*@dev",
    "ycms/module-main": "*@dev"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~4.0",
    "phpspec/phpspec": "~2.1",
    "laracasts/generators": "dev-master"
  },
  "autoload": {
    "files": [
    ],
    "classmap": [
      "source/database",
      "source/app/Facades"
    ],
    "psr-4": {
      "YCMS\\": "source/app/",
      "App\\": "source/modules/",
      "YC\\":"app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "source/tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-root-package-install": [
      "php -r \"copy('source/.env.example', 'source/.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "prefer-stable": true
}
